package selfProgramming;

class Casting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int total = 50;
		float result = (float) total / 6;
		double latest = result;
		
		System.out.print(result); // value in flaoting point
		System.out.print("\n"+ latest); //in double type
	}
	

}
